 disabled 禁止\r\n\r\n        el-submenu \r\n            下拉菜单 \r\n\r\n     -->\r\n    <el-menu\r\n      :default-active=\"$route.path\"\r\n      class=\"el-menu-demo\"\r\n      mode=\"horizontal\"\r\n      background-color=\"#545c64\"\r\n      text-color=\"#fff\"\r\n      active-text-color=\"#ffd04b\"\r\n      router\r\n      @select=\"selectMenu\"\r\n    >\r\n      <el-menu-item index=\"/\">首页</el-menu-item>\r\n      <el-menu-item index=\"/news\">新闻</el-menu-item>\r\n      <el-menu-item index=\"/about\">我的</el-menu-item>\r\n      <el-menu-item index=\"/travel\">旅游</el-menu-item>\r\n      <!-- 右侧内容 -->\r\n      <div class=\"nav-right\">\r\n        <el-button v-if=\"!userinfo.username\" size=\"small\" @click=\"login\">登录</el-button>\r\n        <template v-else>\r\n          <span>欢迎：{{ userinfo.username }}</span>\r\n          <i class=\"el-icon-switch-button\" @click=\"loginout\"></i>\r\n        </template>\r\n      </div>\r\n    </el-menu>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { mapState, mapMutations } from \"vuex\";\r\nexport default {\r\n  data() {\r\n    return {\r\n      activeIndex: \"/\",\r\n    };\r\n  },\r\n  computed: {\r\n    ...mapState(\"LoginModule\", [\"userinfo\"]),\r\n  },\r\n  methods: {\r\n    ...mapMutations(\"LoginModule\", [\"clearUser\"]),\r\n    //退出登录\r\n    loginout() {\r\n      //1. 清空vuex用户信息  2. 清空本地数据\r\n      this.clearUser();\r\n      localStorage.removeItem(\"userinfo\");\r\n      //跳转当前的url地址路径\r\n      console.log('this.$route.path',this.$route.path);\r\n      if (this.$route.path!== \"/\") {\r\n        this.$router.push(\"/login\");\r\n      }\r\n    },\r\n    //登录\r\n    login(){\r\n      this.$router.push('/login')\r\n    },\r\n\r\n    selectMenu(index, path) {\r\n      //   console.log(this);\r\n      //   localStorage.setItem('index',index);\r\n    },\r\n  },\r\n  created() {\r\n    //   let index  = localStorage.getItem('index')\r\n    //   if(index){\r\n    //       this.activeIndex =index;\r\n    //   }\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang='less' scoped>\r\n.nav {\r\n  background: #545c64;\r\n}\r\n.el-menu-demo {\r\n  width: 1200px;\r\n  margin: 0 auto;\r\n}\r\n.el-menu.el-menu--horizontal {\r\n  border-bottom: 0;\r\n}\r\n.nav-right {\r\n  float: right;\r\n  line-height: 60px;\r\n  color: #fff;\r\n  span {\r\n    padding-right: 10px;\r\n  }\r\n  i {\r\n    font-size: 20px;\r\n    vertical-align: middle;\r\n    cursor: pointer;\r\n  }\r\n}\r\n</style>"]}]}